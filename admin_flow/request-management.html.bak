    <!-- Edit userjs -->
    <script>

        function filterTableFunction(tab, category) {
            const tabData = document.querySelectorAll('.requestmanage-switchtab');
            tabData.forEach((item, index) => {
                if (index === tab) {
                    item.classList.add('requestmanage-activebar');
                }
                else {
                    item.classList.remove('requestmanage-activebar');
                }
            })


            var rows = document.querySelectorAll('.filterable');
            rows.forEach(function (row) {
                if (category === 'All' || row.getAttribute('data-category') === category) {
                    row.style.display = '';
                } else {
                    row.style.display = 'none';
                }
            });

            var tablinks = document.querySelectorAll('.tablink');
            tablinks.forEach(function (tablinks) {
                if (tablinks.textContent.toLowerCase() === category.toLowerCase()) {
                    tablinks.classList.add('active');
                } else {
                    tablinks.classList.remove('active');
                }
            });
        }
        // function switchTab(tab) {
        //   const tabData = document.querySelectorAll('.requestmanage-switchtab');
        //   tabData.forEach((item, index) => {
        //     if (index === tab) {
        //       item.classList.add('requestmanage-activebar');
        //     }
        //     else {
        //       item.classList.remove('requestmanage-activebar');
        //     }
        //   })
        // }



        function showDropdown(data) {
            const unhideOptions = ['salesagentoptionsfield', 'buildingoptionsfield', 'unittypeoptionsfield', 'unitstatusoptionsfield'];
            const removeOptions = unhideOptions.filter((item) => item !== data);
            removeOptions.forEach((list) => {
                document.getElementById(list).classList.remove("dropdownshown");
            })
            document.getElementById(data).classList.add("dropdownshown");
        }

        function filterFunction(filterdata, dropdowndata) {
            var input, filter, div, labels, i, txtValue;
            input = document.getElementById(filterdata);
            filter = input.value.toUpperCase();
            div = document.getElementById(dropdowndata);
            labels = div.getElementsByTagName("label");
            for (i = 0; i < labels.length; i++) {
                txtValue = labels[i].textContent || labels[i].innerText;
                if (txtValue.toUpperCase().indexOf(filter) > -1) {
                    labels[i].style.display = "";
                } else {
                    labels[i].style.display = "none";
                }
            }
        }

        function updateSelected(fieldoptions, displayedoptions) {
            var checkboxes = document.getElementsByClassName(fieldoptions);
            var selectedOptionsDiv = document.getElementById(displayedoptions);

            // Clear all dynamically added children
            selectedOptionsDiv.innerHTML = '';

            for (var i = 0; i < checkboxes.length; i++) {
                if (checkboxes[i].checked) {
                    (function (checkbox) {
                        var optionDiv = document.createElement('div');
                        optionDiv.className = 'selected-option';
                        optionDiv.textContent = checkbox.value;

                        optionDiv.style.color = '#ffffff';

                        var removeIconDiv = document.createElement('div');
                        removeIconDiv.className = 'remove-icon';
                        removeIconDiv.textContent = 'X';

                        removeIconDiv.style.color = '#ffffff';
                        removeIconDiv.style.cursor = 'pointer';
                        removeIconDiv.style.marginLeft = '10px';

                        // Add onclick event to remove the selected option and uncheck the checkbox
                        removeIconDiv.onclick = function () {
                            optionDiv.remove();
                            checkbox.checked = false;
                        };

                        optionDiv.appendChild(removeIconDiv);
                        selectedOptionsDiv.appendChild(optionDiv);
                    })(checkboxes[i]);
                }
            }
        }

        window.onclick = function (event) {
            if (!event.target.matches('#salesagentfilterInput') && !event.target.closest('.dropdown-content')) {
                document.getElementById("salesagentoptionsfield").classList.remove("dropdownshown");
            }
            if (!event.target.matches('#buildingfilterInput') && !event.target.closest('.dropdown-content')) {
                document.getElementById("buildingoptionsfield").classList.remove("dropdownshown");
            }
            if (!event.target.matches('#unittypefilterInput') && !event.target.closest('.dropdown-content')) {
                document.getElementById("unittypeoptionsfield").classList.remove("dropdownshown");
            }
            if (!event.target.matches('#unitstatusfilterInput') && !event.target.closest('.dropdown-content')) {
                document.getElementById("unitstatusoptionsfield").classList.remove("dropdownshown");
            }
        }

        function handleInputChange(priceInputvalue, rangeInputvalue, rangevalue) {
            for (let i = 0; i < priceInputvalue.length; i++) {
                priceInputvalue[i].addEventListener("input", e => {
                    // Parse min and max values of the range input
                    let minp = parseInt(priceInputvalue[0].value);
                    let maxp = parseInt(priceInputvalue[1].value);
                    let diff = maxp - minp;

                    if (minp > maxp - priceGap) {
                        priceInputvalue[0].value = maxp - priceGap;
                        minp = maxp - priceGap;

                        if (minp < 0) {
                            priceInputvalue[0].value = 0;
                            minp = 0;
                        }
                    }

                    // Check if the price gap is met and max price is within the range
                    if (diff >= priceGap && maxp <= rangeInputvalue[1].max) {
                        if (e.target.className === "min-input") {
                            rangeInputvalue[0].value = minp;
                            let value1 = rangeInputvalue[0].max;
                            rangevalue.style.left = `${(minp / value1) * 100}%`;
                        } else {
                            rangeInputvalue[1].value = maxp;
                            let value2 = rangeInputvalue[1].max;
                            rangevalue.style.right = `${100 - (maxp / value2) * 100}%`;
                        }
                    }
                });

                // Add event listeners to range input elements
                rangeInputvalue[i].addEventListener("input", e => {
                    let minVal = parseInt(rangeInputvalue[0].value);
                    let maxVal = parseInt(rangeInputvalue[1].value);
                    let diff = maxVal - minVal;

                    // Check if the price gap is exceeded
                    if (diff < priceGap) {
                        // Check if the input is the min range input
                        if (e.target.className === "min-range") {
                            rangeInputvalue[0].value = maxVal - priceGap;
                        } else {
                            rangeInputvalue[1].value = minVal + priceGap;
                        }
                    } else {
                        // Update price inputs and range progress
                        priceInputvalue[0].value = minVal;
                        priceInputvalue[1].value = maxVal;
                        rangevalue.style.left = `${(minVal / rangeInputvalue[0].max) * 100}%`;
                        rangevalue.style.right = `${100 - (maxVal / rangeInputvalue[1].max) * 100}%`;
                    }
                });
            }
        }

        // Find all sets of input elements
        const allPriceInputValues = document.querySelectorAll(".price-input input");
        const allRangeInputValues = document.querySelectorAll(".range-input input");
        const allRangeValues = document.querySelectorAll(".slider-container .price-slider");

        // Set the price gap
        let priceGap = 1;

        // Handle input changes for all sets of fields
        for (let i = 0; i < allPriceInputValues.length; i += 2) {
            handleInputChange(
                [allPriceInputValues[i], allPriceInputValues[i + 1]],
                [allRangeInputValues[i], allRangeInputValues[i + 1]],
                allRangeValues[i / 2]
            );
        }

    </script>

